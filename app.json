{
  "expo": {
    "name": "SarangaAyurveda",
    "slug": "saranga-ayurveda-fresh",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "sdkVersion": "54.0.0",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.AyurvedaSaranga.myapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sarangaayurveda.fresh",
      "versionCode": 2,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4941458a-d243-4bad-99dd-40258896396b"
        },
        "EXPO_PUBLIC_API_BASE_URL": "https://ayurveda-saranga-backend.vercel.app/api",
        "EXPO_PUBLIC_WS_URL": "wss://ayurveda-saranga-backend.vercel.app/ws",
        "EXPO_PUBLIC_USE_LOCAL": "0",
        "EXPO_PUBLIC_DEBUG_API": "0"
    }
  }
}
